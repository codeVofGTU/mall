<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.codev.mall.orm.mapper.MallorderMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.codev.mall.orm.entity.Mallorder">
        <id column="order_seq" property="orderSeq" />
        <result column="order_name" property="orderName" />
        <result column="order_pmt" property="orderPmt" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        order_seq, order_name, order_pmt
    </sql>

</mapper>
