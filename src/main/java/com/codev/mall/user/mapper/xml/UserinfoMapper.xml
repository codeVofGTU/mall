<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.codev.mall.user.mapper.UserinfoMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.codev.mall.user.entity.Userinfo">
        <id column="seq" property="seq" />
        <result column="username" property="username" />
        <result column="password" property="password" />
        <result column="tel" property="tel" />
        <result column="memberid" property="memberid" />
        <result column="accout" property="accout" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        seq, username, password, tel, memberid, accout
    </sql>

</mapper>
